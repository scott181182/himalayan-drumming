{
    "name": "himalayan-drumming-research-server",
    "version": "1.0.0",
    "main": "dist/index.js",
    "author": "Scott Fasone",
    "license": "MIT",
    "scripts": {
        "lint": "eslint",
        "generate": "yarn generate:prisma && yarn generate:nexus",
        "generate:prisma": "prisma generate",
        "generate:nexus": "NEXUS_GEN=1 ts-node --transpile-only ./src/graphql/index.ts",
        "generate:nexus:watch": "NEXUS_GEN=1 ts-node-dev --transpile-only --no-notify ./src/graphql/index.ts",
        "db:migrate": "prisma migrate dev",
        "db:seed": "ts-node ./src/scripts/db_seed.ts",
        "db:scan": "ts-node ./src/scripts/db_scan.ts",
        "build": "tsc -p . --pretty",
        "start": "node ."
    },
    "dependencies": {
        "@apollo/server": "^4.9.2",
        "@prisma/client": "^5.2.0",
        "axios": "^1.4.0",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "graphql": "^16.7.1",
        "module-alias": "^2.2.3",
        "nexus": "^1.3.0",
        "prisma": "^4.16.2",
        "tsconfig-paths": "^4.2.0",
        "zod": "^3.22.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/node": "18.17.1",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "eslint": "^8.44.0",
        "eslint-import-resolver-typescript": "^3.6.0",
        "eslint-plugin-import": "^2.26.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6"
    },
    "_moduleAliases": {
        "@": "./dist"
    }
}
